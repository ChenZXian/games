<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.android.boot">
  <application
    android:allowBackup="false"
    android:icon="@mipmap/app_icon"
    android:label="@string/app_name"
    android:theme="@style/Theme.Game">
    <activity android:name="com.android.boot.MainActivity" android:exported="true" android:screenOrientation="sensor">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>
</manifest>
